{
  "batch_size":1,
  "num_epochs":200,
  "sorta_epoch":1,
  "ckpt":"examples/vc/vcc2018/ckpts/StarGAN/",
  "summary_dir":"examples/vc/vcc2018/ckpts/StarGAN/event",
  "solver_gpu":[3],
  "solver_type":"vc",
  "solver_config":{
    "clip_norm":100,
    "log_interval":10,
    "enable_tf_function":true
  },

  "model":"stargan",
  "num_classes": null,
  "pretrained_model": null,
  "model_config":{
    "codedsp_dim":36,
    "lambda_cycle":10,
    "lambda_identity":5,
    "lambda_classifier":3,

    "optimizer_config":{
      "d_model":512,
      "warmup_steps":8000,
      "k":0.12,
      "decay_steps": 200000,
      "decay_rate": 0.1
    }
  },

  "inference_config":{
    "model_avg_num": 0,
    "fs": 22050
  },

  "dataset_builder": "voice_conversion_dataset",
  "num_data_threads": 1,
  "trainset_config":{
    "data_csv": "examples/vc/vcc2018/data_numpy/train.csv",
    "codedsp_dim":36,
    "cmvn_file":"examples/vc/vcc2018/data/cmvn",
    "input_length_range":[10, 8000],
    "enable_load_from_disk": true,
    "fs": 22050,
    "fft_size": 1024
  },
  "devset_config":{
    "data_csv": "examples/vc/vcc2018/data_numpy/dev.csv",
    "codedsp_dim":36,
    "cmvn_file":"examples/vc/vcc2018/data/cmvn",
    "input_length_range":[10, 8000],
    "enable_load_from_disk": true,
    "fs": 22050,
    "fft_size": 1024
  },
  "testset_config":{
    "data_csv": "examples/vc/vcc2018/data_numpy/test.csv",
    "codedsp_dim":36,
    "cmvn_file":"examples/vc/vcc2018/data/cmvn",
    "input_length_range":[10, 8000],
    "enable_load_from_disk": true,
    "fs": 22050,
    "fft_size": 1024,
    "num_speakers": 12
  }
}
